<html>
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">
<title>Take Picture</title>
<!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
<!--[if lt IE 9]>
<script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
<script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
<![endif]-->  
<link href="bootstrap-3.3.7/css/bootstrap.min.css" rel="stylesheet">
<link rel="stylesheet" href="css/style.css"> 
<link rel="stylesheet" href="css/sweetalert.css">
</head>

<body ng-app="licensePlate" style="background-color:#ababab;" >
<div ng-controller="licensePlateCtrl" ng-init=init()>
<div class="container-fluid p-x-0">
  <nav class="navbar navbar-inverse">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="#">
        <img alt="Brand" src="images/car-64.png" style="height: 30px; display: inline-block; margin-top: -5px">
        License Plate Tracker
      </a>
    </div>
  </div>
</nav>
</div>
  <div class="container">
    <div class="row">

      <div class="col-md-offset-2 col-md-4">
        <video id="camFeed" width="320" height="240" autoplay></video>
      </div>
      <div class="col-md-4">
        <canvas id="image" name="image" width="320" height="240"></canvas>
      </div>
      
    </div>

    <div class="row">
      <div class="form-group cty" style="width:64%">
        <label for="sel1">Select Country:</label>
        <!--<select ng-model="getCountry" class="form-control" id="sel1">
          <option ng-selected="getCountry" value="us">USA</option>
          <option value="eu">Europe</option>
        </select>-->
        <select 
          ng-model="selectedOption" 
          ng-options="option.name for option in options" class="form-control" id="sel1">
      </select>

      </div>
      <div class="sub-btns">
        <div class="col-md-offset-2 col-md-4">
          <button type="button" class="btn btn-danger btn-block btn-grp" ng-click="takePhoto()">Take Picture</button>
        </div>
        <div class="col-md-4">
          <button type="button" class="btn btn-success btn-block btn-grp" ng-click="uploadPicture()">Submit</button>
        </div>
      </div>
    </div>

    <!-- <form enctype="multipart/form-data" action="/upload" method="post">
      <input type="file" name="photo" multiple />
      <input type="submit" value="Upload Image" name="submit" class="btn btn-success" />
    </form> -->
  </div>
</div>

<script type="text/javascript" src="js/jquery-3.1.1.min.js"></script>
<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.9/angular.min.js"></script>      
<script type="text/javascript" src="bootstrap-3.3.7/js/bootstrap.min.js"></script> 
<!--<script src="https://webrtc.github.io/adapter/adapter-latest.js"></script>-->
<script src="js/sweetalert.min.js"></script> 
<script type="text/javascript" src="js/capture.js"></script>

</body>
</html>
